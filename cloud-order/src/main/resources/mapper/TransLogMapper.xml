<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jaswine.mapper.TransLogMapper">


    <select id="selectByTid" resultType="com.jaswine.bean.pojo.RocketMqTransLog">
        SELECT *
        FROM rocketmq_trasnaction_log
        WHERE trans_id = #{tid}
    </select>
</mapper>
    